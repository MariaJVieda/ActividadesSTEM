#define LED_PIN_1 2
#define LED_PIN_2 3
#define LED_PIN_3 4
#define LED_PIN_4 5
#define LED_PIN_5 6

void setup() {
  // Configurar los pines de los LEDs como salidas
  pinMode(LED_PIN_1, OUTPUT);
  pinMode(LED_PIN_2, OUTPUT);
  pinMode(LED_PIN_3, OUTPUT);
  pinMode(LED_PIN_4, OUTPUT);
  pinMode(LED_PIN_5, OUTPUT);
}

void loop() {
  // Encender el primer LED
  digitalWrite(LED_PIN_1, HIGH);
  delay(1000);

  // Encender el segundo LED
  digitalWrite(LED_PIN_2, HIGH);
  delay(1000);

  // Encender el tercer LED
  digitalWrite(LED_PIN_3, HIGH);
  delay(1000);

  // Encender el cuarto LED
  digitalWrite(LED_PIN_4, HIGH);
  delay(1000);

  // Encender el quinto LED
  digitalWrite(LED_PIN_5, HIGH);
  delay(1000);

  // Mantener todos los LEDs encendidos durante 10 segundos
  delay(10000);

  // Apagar todos los LEDs
  digitalWrite(LED_PIN_1, LOW);
  digitalWrite(LED_PIN_2, LOW);
  digitalWrite(LED_PIN_3, LOW);
  digitalWrite(LED_PIN_4, LOW);
  digitalWrite(LED_PIN_5, LOW);

  // Esperar 1 segundo antes de reiniciar el ciclo
  delay(1000);
}
